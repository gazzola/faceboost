{
  "name": "FaceBoost - SociUnify ®",
  "version": "1.1",
  "description": "Um plugin para o Facebook (**sem necessidade de Login**) apenas com notícias rápidas, limpas e eficiente. ",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "FaceBoost - SociUnify ®",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "icons": { "16": "icon.png", "48": "icon.png", "128": "icon.png" },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
